int main () {
	double smallR = 0;
	double largeR = 0;
	double ε = 0;
	cout << "Small Radius:"<< endl;
	cin >> smallR >> endl;
	cout << "Large Radius:"<< endl;
	cin >> largeR >> endl;
	cout << "Permittivity:"<< endl;
	cin >> ε >> endl;
	cout << "Z0 = " << getZ0(smallR, largeR, ε) << endl;
}

double getZ0(r, R, ε){
	double F = getF(r,R);
	double eta = getEta(ε);
	double Z0 = F*eta;
	return Z0;
}

double getF(r,R){
	double F = (1 / (2 * M_PI)) * log(R / r);
	return F;
}

